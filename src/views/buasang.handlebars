<body>
<div class="section-title">
			<h2>Bữa sáng</h2>
</div>

<div class="mt-4">
  <div class="row">
    {{#each user}}
      <div class="col-sm-6 col-lg-4">
        <div class="card card-course-item">
          <a href="/BuaSang/{{this.SLUG}}">
            <img class="card-img-top" src="{{this.IMAGE}}" alt="{{this.name}}">
          </a>
          <div class="card-body">
            <a href="/BuaSang/{{this.SLUG}}">
              <h5 class="card-title">{{this.NAME}}</h5>
            </a>
            <p class="card-text">{{this.INTRODUCE}}.</p>
            <div class="blockheart">
              <div class="heart" id="{{this.ID}}"></div>
              <div class="animation-heart" id=""></div>
            </div>
          </div>
        </div>
      </div>
    {{/each}}
  </div>
</div>
</body>