<div class="section-title">
    <h2>{{ currentTitle }}</h2>
</div>
<div class="mt-4">
    <div class="col-sm-6 col-lg-4">
      <div class="card card-profile-item" data-name="{{userData.NAME}}" data-email="{{userData.EMAIL}}" data-phone="{{userData.PHONE}}" data-address="{{userData.ADDRESS}}">
        <img class="card-img-top" src="{{userData.AVATAR}}" alt="{{userData.NAME}}">
        <div class="card-body">
          <h5 class="card-title">{{userData.NAME}}</h5>
          <p class="card-text">Email: {{userData.EMAIL}}</p>
          <p class="card-text">Số điện thoại: {{userData.PHONE}}</p>
          <p class="card-text">Địa chỉ: {{userData.ADDRESS}}</p>
        </div>
      </div>
    </div>
    <div class="col-sm-12"> 
      <div class="text-center"> 
        <h2>Mẹo vặt yêu thích</h2>
      </div>
    </div>
  <div class="row"> 
    {{#each userFavouriteMeo}}
    <div class="col-sm-6 col-lg-4">
      <div class="card card-course-item" data-slug="{{this.SLUG}}" data-name="{{this.NAME}}" data-image="{{this.IMAGE}}">
        <a href="/MeoVat/{{this.SLUG}}">
          <img class="card-img-top" src="{{this.IMAGE}}" alt="{{this.NAME}}">
        </a>
        <div class="card-body">
          <a href="/MeoVat/{{this.SLUG}}">
            <h5 class="card-title">{{this.NAME}}</h5>
          </a>
        </div>
      </div>
    </div>
    {{/each}}
  </div> 
</div> 
<div class="col-sm-12"> 
      <div class="text-center"> 
        <h2>Món ăn yêu thích</h2>
      </div>
      <div class="text-center"> 
        <h3>Buổi sáng</h3>
      </div>
    </div>
  <div class="row"> 
    {{#each userFavouriteMonAnBuoiSang}}
    <div class="col-sm-6 col-lg-4">
      <div class="card card-course-item" data-slug="{{this.SLUG}}" data-name="{{this.NAME}}" data-image="{{this.IMAGE}}">
        <a href="/BuaSang/{{this.SLUG}}">
          <img class="card-img-top" src="{{this.IMAGE}}" alt="{{this.NAME}}">
        </a>
        <div class="card-body">
          <a href="/BuaSang/{{this.SLUG}}">
            <h5 class="card-title">{{this.NAME}}</h5>
          </a>
        </div>
      </div>
    </div>
    {{/each}}
  </div> 
  <div class="text-center"> 
        <h3>Buổi trưa</h3>
      </div>
  <div class="row"> 
    {{#each userFavouriteMonAnBuoiTrua}}
    <div class="col-sm-6 col-lg-4">
      <div class="card card-course-item" data-slug="{{this.SLUG}}" data-name="{{this.NAME}}" data-image="{{this.IMAGE}}">
        <a href="/BuaTrua/{{this.SLUG}}">
          <img class="card-img-top" src="{{this.IMAGE}}" alt="{{this.NAME}}">
        </a>
        <div class="card-body">
          <a href="/BuaTrua/{{this.SLUG}}">
            <h5 class="card-title">{{this.NAME}}</h5>
          </a>
        </div>
      </div>
    </div>
    {{/each}}
  </div>
</div> 
